<% include partials/header.ejs %>
<h1>VJReddit</h1>
<form method="GET" action="/newpost">
    <button class="btn btn-primary">Post</button>
</form>
<div class="container">
    <div class="row">
     <% posts.forEach(function(post){ %>
        <div class="col-lg-6 col-md-6">
            <img src="<%= post.image %>" >
            <div>
                    <p><%= post.username %></p>
                    <p><%= post.description %></p>
            </div>
        </div>
     <% }) %>
    </div>
</div>
<% include partials/footer.ejs %>